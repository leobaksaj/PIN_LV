<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <EditText
        android:id="@+id/textInputLayout3"
        android:layout_width="391dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="223dp"
        android:background="@drawable/custom_input"
        android:hint="@string/unesiteImeKolegija">

    </EditText>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginStart="160dp"
        android:layout_marginTop="16dp"
        android:maxWidth="150dp"
        android:maxHeight="150dp"
        android:layout_centerHorizontal="true"
        app:srcCompat="@mipmap/ic_launcher" />

    <EditText
        android:id="@+id/textInputLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="166dp"
        android:background="@drawable/custom_input"
        android:hint="@string/unesiteImeProfesora"
        >

    </EditText>

    <Button
        android:id="@+id/buttonIme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textInputLayout2"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="543dp"
        android:layout_marginEnd="14dp"
        android:text="Dalje"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout" />

    <View
        android:id="@+id/gornjacrta"
        android:layout_width="wrap_content"
        android:layout_height="3dp"
        android:layout_below="@+id/textInputLayout3"
        android:layout_alignParentStart="true"
        android:layout_marginStart="-5dp"
        android:layout_marginTop="19dp"
        android:background="@color/purple_700" />

    <View
        android:id="@+id/donjacrta"
        android:layout_width="wrap_content"
        android:layout_height="3dp"
        android:layout_below="@+id/textView8"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="16dp"
        android:background="@color/purple_700"
        />

    <EditText
        android:id="@+id/editTextNumber"
        android:layout_width="102dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="227dp"
        android:layout_marginTop="294dp"
        android:layout_toEndOf="@+id/textView8"
        android:inputType="number" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="21dp"
        android:layout_marginTop="309dp"
        android:text="@string/unesiteGodinuStudija"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/donjacrta"
        android:layout_alignParentStart="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="13dp"
        android:layout_marginTop="74dp"
        android:text="@string/brSati"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView8"
        android:layout_marginStart="24dp"
        android:layout_marginTop="27dp"
        android:layout_toEndOf="@+id/textView9"
        android:text="@string/predavanja"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18dp"
        android:layout_below="@+id/textView8"
        android:layout_marginStart="300dp"
        android:layout_marginTop="30dp"
        android:text="@string/vjezbe" />

    <EditText
        android:id="@+id/editTextNumber2"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_marginStart="21dp"
        android:layout_marginTop="34dp"
        android:layout_marginBottom="267dp"
        android:layout_toEndOf="@+id/textView9"
        android:inputType="number" />

    <EditText
        android:id="@+id/editTextNumber3"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView7"
        android:layout_marginStart="280dp"
        android:layout_marginTop="34dp"
        android:ems="10"
        android:inputType="number" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="19dp"
        android:text="@string/dalje1" />

</RelativeLayout>